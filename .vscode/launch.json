{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Serverless",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless",
      "args": ["offline", "--httpPort", "4000", "--noTimeout", "--stage", "dev"],
    }
  ]
}